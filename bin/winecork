#!/usr/bin/perl

use Modern::Perl;

use FindBin;
use lib "$FindBin::Bin/../lib";

use WineCork::Main;

=head1 NAME

winecork

=head1 SYNOPSIS

    $ bin/winecork

=cut

my $cork = WineCork::Main->new();
exit $cork->main();

